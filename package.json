{
  "name": "quantifiedtalk",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && npm run bundle && cordova run android",
    "tsc": "tsc",
    "bundle": "webpack --config webpack.config.js --source-map"
  },
  "devDependencies": {
    "@types/cordova": "^0.0.34",
    "awesome-typescript-loader": "^4.0.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "author": "mathilde buenerd",
  "license": "ISC",
  "keywords": [
    "app",
    "quantified-self",
    "sentiment",
    "analysis",
    "assistant",
    "personal",
    "assistant"
  ],
  "dependencies": {
    "cordova-android": "^7.0.0",
    "cordova-browser": "^5.0.3",
    "cordova-plugin-speechrecognition": "^1.2.0",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-speechrecognition": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}