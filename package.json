{
  "name": "quantifiedtalk",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && npm run bundle && cordova run android",
    "tsc": "tsc",
    "bundle": "webpack --config webpack.config.js --source-map"
  },
  "devDependencies": {
    "@types/cordova": "^0.0.34",
    "@types/phonegap-plugin-push": "^2.1.2",
    "awesome-typescript-loader": "^4.0.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "author": "mathilde buenerd",
  "license": "ISC",
  "keywords": [
    "app",
    "quantified-self",
    "sentiment",
    "analysis",
    "assistant",
    "personal",
    "assistant"
  ],
  "dependencies": {
    "ajv": "^6.4.0",
    "cordova-android": "^7.0.0",
    "cordova-browser": "^5.0.3",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-contacts-phonenumbers": "0.0.10",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-media": "^5.0.2",
    "cordova-plugin-sms": "^1.0.5",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sms-plugin": "^0.1.11",
    "darktriad": "^1.0.0",
    "phonegap-plugin-push": "^2.1.3",
    "phonegap-plugin-speech-recognition": "^0.3.0",
    "sentiment-multilang": "0.0.2",
    "translate": "^1.0.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "phonegap-plugin-push": {
        "FCM_VERSION": "11.6.2"
      },
      "cordova-plugin-media": {},
      "phonegap-plugin-speech-recognition": {},
      "cordova-sms-plugin": {},
      "cordova-plugin-sms": {},
      "com.dbaq.cordova.contactsPhoneNumbers": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}